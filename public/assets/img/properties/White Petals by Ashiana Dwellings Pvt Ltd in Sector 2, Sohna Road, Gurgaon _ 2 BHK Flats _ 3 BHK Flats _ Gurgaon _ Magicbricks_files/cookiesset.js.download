		//hide  
}


function createCookie(name, value, hour){
	var expires = "";
	if(name=='userType' && value.indexOf(",")>0) {
		value=value.substring(0, value.indexOf(","));
	}
	if (hour) {
		var date = new Date();
	    date.setTime(date.getTime()+(hour*60*60*1000));
	    expires ="";
	}
	document.cookie = name+"="+value+expires+"; path=/";
}

function fillNameAndEmailFromCookies(){
                var tempName = readCookie("userName");
                if(!tempName){
                                tempName = readCookie("userNameflSlider");
                }
                var tempEmail = readCookie("userEmail");
                if(!tempEmail){
                                tempEmail = readCookie("userEmailflSlider");
                }
                var tempMobile = readCookie("userMobile");
                if(!tempMobile){
                                tempMobile = readCookie("userMobileflSlider");
                }
                if(tempName && tempEmail && tempMobile ){
                                document.getElementsByName("Name")[0].value = tempName;
                                document.getElementsByName("Mobile")[0].value = tempMobile; 
                                tempEmail = tempEmail.replace(/(\r\n|\n|\r)/gm,"");
                tempEmail= tempEmail.replace(new RegExp('\"', 'g'),"");
                                document.getElementsByName("Email")[0].value = tempEmail;
                }
}

window.onload = fillNameAndEmailFromCookies();

	
